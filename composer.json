{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.2",
        "laravel/framework": "^12.0",
        "laravel/pulse": "^1.4",
        "laravel/telescope": "^5.14",
        "laravel/tinker": "^2.9",
        "predis/predis": "^3.2",
        "resend/resend-laravel": "^0.19.0",
        "spatie/laravel-permission": "^6.0",
        "stancl/tenancy": "^3.5.1",
        "stechstudio/filament-impersonate": "^3.16",
        "stephenjude/filament-debugger": "^4.0",
        "vonage/client": "^4.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "jasonmccreary/laravel-test-assertions": "^2.8",
        "larastan/larastan": "^3.0",
        "laravel-shift/blueprint": "^2.12",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.22",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpro/grumphp": "^2.13",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.0.1",
        "sebastian/phpcpd": "^2.0",
        "squizlabs/php_codesniffer": "^3.13"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php vendor/bin/grumphp git:init"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:work --tries=1\" \"npm run dev\" --names=server,queue,vite"
        ],
        "post-install-cmd": [
            "@php vendor/bin/grumphp git:init"
        ],
        "grumphp": "grumphp run",
        "lint": [
            "pint --test",
            "phpstan analyse --memory-limit=2G"
        ],
        "lint:fix": [
            "pint"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpro/grumphp": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
